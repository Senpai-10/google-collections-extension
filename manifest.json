{
    "name": "gce",
    "manifest_version": 2,
    "version": "1.0",
    "description": "next and previous page in google chrome collections",
    "browser_action": {
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ]
}